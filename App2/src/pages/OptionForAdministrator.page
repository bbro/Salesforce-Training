				<apex:commandLink value="  List of cities    " action="{!showListOfCities}" /> <br/>
				<apex:commandLink value=" --->  Add new city    " action="{!showAddNewCityView}" /> <br/>
				<apex:commandLink value="  List of stations    " action="{!showListOfStations}" /> <br/>
				<apex:commandLink value=" --->  Add new stations    " action="{!showAddNewStationView}" /> <br/>
				<apex:commandLink value="  List of routes    " action="{!showListOfRoutes}" /> <br/>
				<apex:commandLink value=" --->  Add new route    " action="{!showAddNewRouteView}" /> <br/>
			</apex:pageBlockSection>
		</apex:pageBlock>
