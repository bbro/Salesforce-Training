	<apex:form >
		<apex:pageBlock >
			OPTIONS FOR ADMINISTRATOR <br/><br /><br />
			<apex:pageBlockSection title="Available options">
				<apex:commandLink value="  List of trains    " action="{!showListOfTrains}" /> <br/>
				<apex:commandLink value=" --->  Add new trains    " action="{!showAddNewTrains}" /> <br/>
				<apex:commandLink value="  List of carriage    " action="{!showListOfCarriage}" /> <br/>
